{
  "name": "ubjs",
  "private": true,
  "repository": "https://git-pub.intecracy.com/unitybase/ubjs",
  "devDependencies": {
    "lodash": "^4",
    "standard": "*",
    "babel-loader": "^6.2.10",
    "babel-core": "^6.21.0",
    "css-loader": "^0.26.1",
    "exports-loader": "^0.6.3",
    "imports-loader": "^0.7.0",
    "style-loader": "^0.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-without-strict": "0.0.4",
    "webpack": "^2.2.0",
    "webpack-bundle-analyzer": "^2.3.1",
    "uglify-js": "^2.7.4",
    "uglifycss": "*",
    "jsdoc": "~3.4.0",
    "lerna": "2.0.0-rc.5"
  },
  "scripts": {
    "clean": "call ./.bin/clean.cmd",
    "build:doc": "jsdoc -c ./jsdoc-conf.json --package ./package.json",
    "build:js": "lerna run build",
    "build:native": "lerna run build:native --concurrency 1",
    "bootstrap": "npm run clean && call ./.bin/bootstrap.cmd"
  },
  "engines": {
    "npm": "^5"
  }
}
