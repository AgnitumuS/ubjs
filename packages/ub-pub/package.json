{
  "name": "@unitybase/ub-pub",
  "version": "4.0.6",
  "description": "UnityBase data layer for Browser client",
  "main": "index.js",
  "config": {
    "ubmodel": "ub-pub",
    "ubmodelispublic": true
  },
  "scripts": {
    "build": "SET NODE_ENV=production && webpack -p",
    "build:doc": "jsdoc -c ./jsdoc-conf.json --package ./package.json",
    "analyse-boundle": "webpack --profile --json > wp_stats.json && webpack-bundle-analyzer ./wp_stats.json",
    "lint": "eslint source test",
    "prepublish": "npm run build",
    "postinstall": "ubcli flow/modelInstalled",
    "test": "npm run test"
  },
  "public": true,
  "publishConfig": {
    "registry": "http://registry.unitybase.info"
  },
  "dependencies": {
    "@unitybase/base": "^4.0.5",
    "lodash": "^4",
    "@unitybase/cryptojs": "^1.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "webpack": "^1.14.0"
  },
  "keywords": [
    "UnityBase"
  ],
  "author": "pavel.mash <pavel.mash@gmail.com> (https://unitybase.info)",
  "license": "(GPL-3.0 OR LGPL-3.0 OR MPL-2.0)"
}
