{
  "name": "@unitybase/adminui-next",
  "version": "1.0.0",
  "description": "UnityBase admin UI extended with Vue + ElementUI",
  "main": "adminui-next.js",
  "browser": {
    "dev": "adminui-next.js",
    "prod": "./dist/adminui-next.min.js"
  },
  "repository": "https://git-pub.intecracy.com/unitybase/ubjs/tree/master/packages/adminui-newx",
  "config": {
    "ubmodel": {
      "name": "adminui-next",
      "isPublic": true
    }
  },
  "scripts": {
    "build": "npm run build:theme && npm run build:js",
    "prepublish": "npm run build",
    "gen-el-vars": "et -i theme/ub-el.scss",
    "build:theme": "npm run build:el && npm run build:css",
    "build:el": "et -c theme/ub-el.scss -o dist",
    "build:css": "cleancss -o ./dist/adminui-next.css ./node_modules/normalize.css/normalize.css ./theme/ub-body.css ./dist/index.css",
    "build:js": "cross-env NODE_ENV=production && webpack",
    "analyse-boundle": "cross-env NODE_ENV=production && webpack --profile --json > wp_stats.json && webpack-bundle-analyzer ./wp_stats.json"
  },
  "files": [
    "dist/adminui-next.css",
    "dist/adminui-next.min.js",
    "dist/fonts"
  ],
  "element-theme": {
    "minimize": true
  },
  "public": true,
  "publishConfig": {
    "registry": "http://registry.unitybase.info"
  },
  "dependencies": {
    "@unitybase/cs-shared": "^5.0.6",
    "@unitybase/ub-pub": "^5.1.0",
    "element-ui": "^2.4.3",
    "vue": "^2.5.16",
    "font-awesome": "^4.7.0",
    "lodash": "^4",
    "systemjs": "^0.20.18",
    "systemjs-plugin-css": "^0.1.32"
  },
  "keywords": [
    "UnityBase",
    "Vue",
    "ElementUI"
  ],
  "author": "UnityBase team (https://unitybase.info)",
  "license": "(GPL-3.0 OR LGPL-3.0 OR MPL-2.0)",
  "devDependencies": {
    "clean-css-cli": "^4.1.11",
    "cross-env": "^5.2.0",
    "element-theme": "^2.0.1",
    "element-theme-chalk": "^2.4.4",
    "normalize.css": "^8.0.0",
    "shx": "^0.2.2",
    "uglify-js": "^3.0.26",
    "webpack-bundle-analyzer": "^2.13.1"
  }
}
