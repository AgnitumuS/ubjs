{
  "caption": "Staff units",
  "description": "List of staff units",
  "documentation": "This dictionary contains list of staff units. Each staff unit links to org_unit by Unity mixin",
  "connectionName": "",
  "sqlAlias": "stunit",
  "cacheType": "Session",
