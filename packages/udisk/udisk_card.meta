{
  "caption": "Files",
  "description": "Files",
  "connectionName": "",
  "sqlAlias": "udiskc",
  "descriptionAttribute": "name",
  "cacheType": "None",
  "attributes": [
    {
      "name": "name",
      "dataType": "String",
      "size": 512,
      "caption": "Name",
      "description": "Name",
      "allowNull": false,
      "isMultiLang": false,
      "customSettings": {
        "permissionEntityName": "udisk_permission",
        "userRole": "udiskPublicUsers",
        "adminRole": "udiskPublicAdmins",
        "adminDenyContentRole": "udiskPublicAdminsDenyContent",
        "diskType": "public"
      }
    },
    {
      "name": "parentID",
      "dataType": "Entity",
      "associatedEntity": "udisk_card",
      "caption": "Folder",
      "description": "Folder",
      "allowNull": true
    },
    {
      "name": "fcomment",
      "dataType": "String",
      "size": 512,
      "caption": "Comment",
      "description": "Comment",
      "allowNull": true
    },
    {
      "name": "isFolder",
      "dataType": "Boolean",
      "caption": "Is folder",
      "description": "Is folder",
      "allowNull": false,
      "defaultValue": "0"
    },
    {
      "name": "ownerID",
      "dataType": "Entity",
      "associatedEntity": "uba_user",
      "caption": "owner",
      "description": "owner",
      "allowNull": false
    },
    {
      "name": "fsize",
      "dataType": "BigInt",
      "caption": "size",
      "description": "size",
      "allowNull": true
    },
    {
      "name": "contentType",
      "dataType": "String",
      "size": 100,
      "caption": "contentType",
      "description": "contentType",
      "allowNull": true
    },
    {
      "name": "isTemporary",
      "dataType": "Boolean",
      "caption": "Temporary",
      "description": "Temporary",
      "allowNull": true,
      "defaultValue": "0"
    },
    {
      "name": "fileData",
      "dataType": "Document",
      "caption": "file",
      "description": "file",
      "allowNull": true,
      "storeName": "udiskStore"
    },
    {
      "name": "objectID",
      "dataType": "BigInt",
      "caption": "Related object ID",
      "defaultView": false
    },
    {
      "name": "objectEntity",
      "dataType": "String",
      "size": 32,
      "caption": "Related object entity name",
      "defaultView": false
    }
  ],
  "mixins": {
    "mStorage": {
      "simpleAudit": true,
      "safeDelete": false
    },
    "tree": {
      "parentAttr": "parentID"
    },
    "rls": {
      "expression": "udisk_card.limitByUser()"
    },
    "softLock": {
      "lockIdentifier": "ID",
      "lockEntity": "udisk_card"
    }
  }
}
