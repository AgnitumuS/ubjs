{
  "connectionName": "",
  "sqlAlias": "ubmSD",
  "cacheType": "None",
  "caption": "Data System Dictionaries",
  "descriptionAttribute": "name",

  "mixins": {
    "mStorage": {
      "simpleAudit": true,
      "safeDelete": true
    },

    "unity": {
      "entity": "ubm_query",
      "attributeList": ["code", "name", "ubql"],
      "defaults": {
        "type": "sysDict"
      }
    }
  },

  "attributes": [
    {
      "name": "code",
      "caption": "Code",
      "dataType": "String",
      "size": 50,
      "allowNull": false,
      "isUnique": true
    },
    {
      "name": "name",
      "caption": "Name",
      "dataType": "String",
      "size": 256,
      "allowNull": false,
      "isMultiLang": true
    },
    {
      "name": "ubql",
      "caption": "UBQL for lookups",
      "description": "UBQL used for lookups",
      "documentation": "Lookup UBQL must contains at last 2 attribute - first one is row ID and second is row caption shown to user",
      "dataType": "Json",
      "allowNull": false
    }
  ]
}
