{
  "caption": "Shortcut",
  "description": "Metadata for build navbars",
  "connectionName": "",
  "sqlAlias": "navsh",
  "cacheType": "None",
  "descriptionAttribute": "caption",
  "documentation": "",
  "attributes": [
    {
      "dataType": "Entity",
      "associatedEntity": "ubm_desktop",
      "caption": "Desktop",
      "description": "",
      "allowNull": false,
      "name": "desktopID"
    },
    {
      "dataType": "Entity",
      "associatedEntity": "ubm_navshortcut",
      "caption": "Shortcut folder",
      "description": "",
      "allowNull": true,
      "name": "parentID"
    },
    {
      "dataType": "String",
      "size": 50,
      "caption": "Code",
      "documentation": "Unique shortcut code. Used for version upgrade",
      "allowNull": false,
      "isUnique": true,
      "name": "code"
    },
    {
      "dataType": "Boolean",
      "caption": "Is this folder?",
      "allowNull": false,
      "defaultValue": "0",
      "name": "isFolder"
    },
    {
      "dataType": "String",
      "size": 255,
      "caption": "Shortcut name",
      "allowNull": false,
      "isMultiLang": true,
      "name": "caption"
    },
    {
      "dataType": "Text",
      "caption": "Command code",
      "name": "cmdCode"
    },
    {
      "dataType": "Boolean",
      "caption": "In new window",
      "description": "Display command result in new window or inside panel",
      "allowNull": false,
      "defaultValue": "0",
      "name": "inWindow"
    },
    {
      "dataType": "Boolean",
      "caption": "Collapsed",
      "description": "Show collapsed at the first start",
      "documentation": "It make sense if isFolder = true",
      "allowNull": false,
      "defaultValue": "0",
      "name": "isCollapsed"
    },
    {
      "dataType": "Int",
      "caption": "â„– order",
      "description": "Display order (in current folder)",
      "allowNull": false,
      "defaultValue": "1000",
      "name": "displayOrder"
    },
    {
      "dataType": "String",
      "size": 50,
      "caption": "icon css class name",
      "allowNull": true,
      "isMultiLang": false,
      "name": "iconCls"
    }
  ],
  "mixins": {
    "mStorage": {
      "simpleAudit": true,
      "safeDelete": true
    },
    "rls": {
      "expression": "`(${$.currentOwner()} OR ${$.currentUserInGroup(ubm_navshortcut,'Admin')} OR ${$.currentUserOrUserGroupInAdmSubtable(ubm_navshortcut)})`"
    },
    "tree": {
      "parentAttr": "parentID"
    }
  }
}