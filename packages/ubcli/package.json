{
  "name": "@unitybase/ubcli",
  "version": "5.5.2",
  "description": "A simple CLI for scaffolding UnityBase projects",
  "repository": "https://git-pub.intecracy.com/unitybase/ubjs/tree/master/packages/ubcli",
  "config": {
    "ubmodel": {}
  },
  "publishConfig": {
    "registry": "https://registry.unitybase.info"
  },
  "bin": {
    "ubcli": "bin/ubcli.js"
