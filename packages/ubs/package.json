{
  "name": "@unitybase/ubs",
  "version": "5.0.6",
  "description": "Reports, User messages (notifications), Settings (aka about:config), Counters",
  "repository": "https://git-pub.intecracy.com/unitybase/ubjs/tree/master/packages/ubs",
  "config": {
    "ubmodel": "UBS"
  },
  "scripts": {
    "test": "ubcli autotest -m UBS -skipModules",
    "build": "cross-env NODE_ENV=production && cd ./public && webpack",
    "build:watch": "cd ./public && webpack --progress --colors --watch",
    "prepublish": "npm run build",
    "analyse-boundle": "cd ./public && webpack --profile --json > wp_stats.json && webpack-bundle-analyzer ./wp_stats.json"
  },
  "public": true,
  "publishConfig": {
    "registry": "http://registry.unitybase.info"
  },
  "main": "ubs.js",
  "dependencies": {
    "@unitybase/adminui-pub": "^5.0.6",
    "@unitybase/base": "^5.0.5",
    "@unitybase/cs-shared": "^5.0.2",
    "@unitybase/pdf": "^5.0.3",
    "@unitybase/ub": "^5.0.6",
    "@unitybase/ub-pub": "^5.0.5",
    "@unitybase/uba": "^5.0.6",
    "@unitybase/xlsx": "^5.0.5",
    "mime-types": "^2.1.14",
    "mustache": "^2.3.0",
    "when": "1.8.1",
    "xmldom": "^0.1.27"
  },
  "devDependencies": {
    "@unitybase/stubs": "^5.0.5",
    "cross-env": "^5.1.1"
  },
  "keywords": [
    "UnityBase",
    "UB4"
  ],
  "author": "UnityBase team (https://unitybase.info)",
  "license": "(GPL-3.0 OR LGPL-3.0 OR MPL-2.0)"
}
