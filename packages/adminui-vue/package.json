{
  "name": "@unitybase/adminui-vue",
  "version": "1.4.0",
  "description": "UnityBase admin UI extended with Vue + ElementUI",
  "main": "adminui-vue.js",
  "browser": {
    "dev": "adminui-vue.js",
    "prod": "./dist/adminui-vue.min.js"
  },
  "repository": "https://git-pub.intecracy.com/unitybase/ubjs/tree/master/packages/adminui-vue",
  "config": {
    "ubmodel": {
      "name": "adminui-vue",
      "isPublic": true
    }
  },
  "scripts": {
    "build": "npm run build:theme && npm run build:js",
    "prepare": "npm run build",
    "gen-el-vars": "et -i theme/ub-el.scss",
    "build:theme": "npm run build:el",
    "build:el": "et -c theme/ub-el.scss -o _el-theme && shx cp ./_el-theme/index.css ./theme/el-theme-compiled.css && shx mkdir -p ./theme/fonts && shx cp ./_el-theme/fonts/* ./theme/fonts",
    "build:js": "webpack --mode production && cleancss -O2 -o ./dist/adminui-vue.min.css ./dist/adminui-vue.css",
    "analyse-boundle": "webpack --mode production --profile --json > wp_stats.json && webpack-bundle-analyzer ./wp_stats.json"
  },
  "element-theme": {
    "browsers": [
      "ie > 10",
      "last 2 versions"
    ],
    "config": "./theme/ub-el.scss",
    "theme": "element-theme-chalk",
    "minimize": false
  },
  "public": true,
  "publishConfig": {
    "registry": "https://registry.unitybase.info"
  },
  "dependencies": {
    "@unitybase/codemirror-full": "^1.2.22",
    "@unitybase/cs-shared": "^5.0.19",
    "@unitybase/systemjs-plugin-vue-ub": "^1.3.2",
    "@unitybase/ub-pub": "^5.2.33",
    "element-ui": "2.5.4",
    "font-awesome": "^4.7.0",
    "jdenticon": "^2.1.1",
    "lodash": "^4",
    "normalize.css": "^8.0.1",
    "systemjs": "^0.20.18",
    "systemjs-plugin-css": "^0.1.32",
    "throttle-debounce": "^2.1.0",
    "vue": "^2.6.8",
    "vue-template-compiler": "^2.6.8"
  },
  "keywords": [
    "UnityBase",
    "Vue",
    "ElementUI"
  ],
  "author": "UnityBase team (https://unitybase.info)",
  "license": "(GPL-3.0 OR LGPL-3.0 OR MPL-2.0)",
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "element-theme": "^2.0.1",
    "element-theme-chalk": "2.5.4",
    "shx": "^0.3.2",
    "vue-loader": "^15.7.0"
  },
  "optionalDependencies": {
    "clean-css-cli": "^4.2.1",
    "cross-env": "^5",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3",
    "webpack-cli": "^3.2.3"
  }
}
