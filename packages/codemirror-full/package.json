    "babel-loader": "^6.2.10",
    "babel-preset-es2015-without-strict": "0.0.4",
    "css-loader": "^0.26.1",
    "exports-loader": "^0.6.3",
    "imports-loader": "^0.7.0",
    "style-loader": "^0.13.1"
  },
  "peerDependencies": {
    "webpack": "^2.2.0",
    "uglify-js": "^2.7.4",
    "uglifycss": "*"
  },
  "keywords": [
