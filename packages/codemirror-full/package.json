{
  "name": "@unitybase/codemirror-full",
  "version": "1.2.0",
  "description": "CodeMirror + plugins + linter",
  "main": "./dist/codemirror-all.min.js",
  "scripts": {
    "build": "SET NODE_ENV=production && webpack",
    "build:watch": "webpack --progress --colors --watch",
    "analyse-boundle": "webpack --profile --json > wp_stats.json && webpack-bundle-analyzer ./wp_stats.json",
    "test": "npm run test"
