{
  "name": "@unitybase/codemirror-full",
  "version": "1.2.2",
  "description": "CodeMirror + plugins + linter",
  "main": "./dist/codemirror-all.min.js",
  "scripts": {
    "build": "SET NODE_ENV=production && webpack",
