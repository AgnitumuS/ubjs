{
  "name": "@unitybase/amqp",
  "version": "1.0.42",
  "description": "This library tries to mimic amqplib nodejs library API but in synchronous mode which is natural for UnityBase",
  "main": "./amqp.js",
  "public": true,
  "publishConfig": {
    "registry": "https://registry.unitybase.info"
  },
  "repository": "https://git-pub.intecracy.com/unitybase/ubjs/tree/master/packages/amqp",
  "scripts": {
    "build": "npm run build:native",
    "build:native": "node ./bin/compile.js",
    "test": "ub tests/main.js",
    "example:hello-pub": "ub ./examples/hello/hello-pub.js",
    "example:hello-sub": "ub ./examples/hello/hello-sub.js",
    "version": "update-changelog && git add CHANGELOG.md"
  },
  "author": "UnityBase team",
  "license": "ISC",
  "dependencies": {
    "assert": "^1.4.1",
    "lodash": "^4"
  },
  "devDependencies": {
    "@unitybase/fpc-utils": "^5.0.8",
    "@unitybase/stubs": "^5.1.0",
    "@unitybase/update-changelog": "^1.0.3"
  },
  "engines": {
    "UnityBase": "^5.0"
  },
  "keywords": [
    "UnityBase",
    "RabbitMQ",
    "amqp",
    "amqplib"
  ]
}
