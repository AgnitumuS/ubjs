{
  "name": "@unitybase/adminui-pub",
  "version": "4.2.45",
  "description": "UnityBase admin UI",
  "main": "adminui.js",
  "repository": "https://git-pub.intecracy.com/unitybase/ubjs/tree/master/packages/adminui-pub",
  "config": {
    "ubmodel": "adminui-pub",
    "ubmodelispublic": true
  },
  "scripts": {
    "build": "npm run build:ext && npm run build:css && npm run build:adminui",
    "build:ext": "call ./bin/buildExt.cmd",
    "build:css": "uglifycss ./_src/css/GridFilters.css ./_src/css/RangeMenu.css ./_src/css/toastr.css ./_src/ux/form/field/BoxSelect.css ./_src/css/ub-badge.css > ./ub-css-all.css",
    "build:adminui": "SET NODE_ENV=production&& webpack",
    "build:watch": "webpack --progress --colors --watch",
    "analyse-boundle": "webpack --profile --json > wp_stats.json && webpack-bundle-analyzer ./wp_stats.json",
    "lint": "eslint source test",
    "prepublish": "npm run build",
    "test": "npm run test"
  },
  "public": true,
  "publishConfig": {
    "registry": "http://registry.unitybase.info"
  },
  "dependencies": {
    "@unitybase/adminui-reg": "^4.1.17",
    "@unitybase/base": "^4.2.26",
    "@unitybase/codemirror-full": "^1.2.12",
    "@unitybase/ub-pub": "^4.4.18",
    "@unitybase/ubm": "^4.0.48",
    "@unitybase/xlsx": "^4.1.4",
    "bluebird": "^3.4.6",
    "bluebird-q": "^2.1.1",
    "codemirror": "^5.20.2",
    "es6-promise": "^4.1.1",
    "file-saver": "^1.3.3",
    "font-awesome": "^4.7.0",
    "js-beautify": "^1.6.12",
    "jszip": "^3.1.3",
    "lodash": "^4",
    "mustache": "^2.3.0",
    "mxgraph-js": "^1.0.1",
    "systemjs": "^0.20.18",
    "systemjs-plugin-css": "^0.1.32",
    "tinymce": "^4.4.3"
  },
  "keywords": [
    "UnityBase",
    "UB4"
  ],
  "author": "UnityBase team (https://unitybase.info)",
  "license": "(GPL-3.0 OR LGPL-3.0 OR MPL-2.0)",
  "devDependencies": {
    "uglify-js": "^3.0.26",
    "uglifycss": "0.0.27"
  }
}
