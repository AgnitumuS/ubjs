{
  "name": "@unitybase/adminui-pub",
  "version": "5.6.25",
  "description": "UnityBase admin UI",
  "main": "adminui.js",
  "browser": {
    "dev": "adminui.js",
    "prod": "./dist/adminui.app.min.js"
  },
  "repository": "https://git-pub.intecracy.com/unitybase/ubjs/tree/master/packages/adminui-pub",
  "config": {
    "ubmodel": {
      "name": "adminui-pub",
      "isPublic": true
    }
  },
  "scripts": {
    "build": "npm run build:prep && npm run build:js && npm run build:theme && npm run build:css && npm run build:adminui",
    "build:prep": "shx mkdir -p ./dist && shx rm -f ./dist/ext-all.*",
    "build:js": "uglifyjs ext-all-debug-w-comments.js -o ./dist/ext-all.min.js --screw-ie8 --prefix relative --compress --mangle",
    "build:css": "cleancss -o ./ub-css-all.css ./_src/css/normalize.css ./_src/css/ub-body.css ./_src/css/GridFilters.css ./_src/css/RangeMenu.css ./_src/css/toastr.css ./_src/ux/form/field/BoxSelect.css ./_src/css/ub-badge.css",
    "build:theme": "cleancss -o ./themes/UBGrayTheme/UBGrayTheme-all.css ./themes/UBGrayTheme/UBGrayTheme-all-debug.css && uglifyjs ./themes/UBGrayTheme/UBGrayTheme-debug.js -o ./themes/UBGrayTheme/UBGrayTheme.js --screw-ie8 --prefix relative --compress --mangle",
    "build:adminui": "cross-env NODE_ENV=production webpack",
    "build:watch": "webpack --progress --colors --watch",
    "analyse-boundle": "webpack --profile --json > wp_stats.json && webpack-bundle-analyzer ./wp_stats.json",
    "lint": "eslint source test",
    "prepublish": "npm run build",
    "test": "npm run test"
  },
  "public": true,
  "publishConfig": {
    "registry": "https://registry.unitybase.info"
  },
  "dependencies": {
    "@unitybase/codemirror-full": "^1.2.19",
    "@unitybase/cs-shared": "^5.0.16",
    "@unitybase/ub-pub": "^5.2.28",
    "@unitybase/ubm": "^5.1.6",
    "@unitybase/xlsx": "^5.0.42",
    "bluebird": "^3.4.6",
    "bluebird-q": "^2.1.1",
    "codemirror": "^5.20.2",
    "es6-promise": "^4.1.1",
    "file-saver": "^1.3.3",
    "font-awesome": "^4.7.0",
    "js-beautify": "^1.6.12",
    "jszip": "2.6.0",
    "lodash": "^4",
    "mustache": "^2.3.0",
    "mxgraph-js": "^1.0.1",
    "systemjs": "^0.20.18",
    "systemjs-hmr": "^2.0.9",
    "systemjs-plugin-css": "^0.1.32",
    "tinymce": "^4.7.13"
  },
  "keywords": [
    "UnityBase",
    "UB5"
  ],
  "author": "UnityBase team (https://unitybase.info)",
  "license": "(GPL-3.0 OR LGPL-3.0 OR MPL-2.0)",
  "devDependencies": {
    "clean-css-cli": "^4.1.11",
    "cross-env": "^5.1.1",
    "shx": "^0.2.2",
    "uglify-js": "^3.0.26"
  }
}
