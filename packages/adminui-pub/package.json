{
  "name": "@unitybase/adminui-pub",
  "version": "5.11.0",
  "description": "UnityBase admin UI",
  "main": "adminui.js",
  "browser": {
    "dev": "adminui.js",
    "prod": "./dist/adminui.app.min.js"
  },
  "repository": "https://git-pub.intecracy.com/unitybase/ubjs/tree/master/packages/adminui-pub",
  "config": {
    "ubmodel": {
      "name": "adminui-pub",
      "isPublic": true
    }
  },
  "scripts": {
    "build": "npm run build:prep && npm run build:js && npm run build:theme && npm run build:css && npm run build:adminui",
    "build:prep": "shx mkdir -p ./dist && shx rm -f ./dist/*.*",
    "build:js": "uglifyjs ext-all-debug-w-comments.js -o ./dist/ext-all.min.js --screw-ie8 --prefix relative --compress --mangle",
    "build:css": "cleancss -O2 -o ./ub-css-all.css ./_src/css/normalize.css ./_src/css/ub-body.css ./_src/css/GridFilters.css ./_src/css/RangeMenu.css ./_src/css/toastr.css ./_src/ux/form/field/BoxSelect.css ./_src/css/ub-badge.css",
    "build:theme": "cleancss -O2 -o ./themes/UBGrayTheme/UBGrayTheme-all.css ./themes/UBGrayTheme/UBGrayTheme-all-debug.css && uglifyjs ./themes/UBGrayTheme/UBGrayTheme-debug.js -o ./themes/UBGrayTheme/UBGrayTheme.js --screw-ie8 --prefix relative --compress --mangle",
    "build:adminui": "webpack --mode production",
    "build:watch": "webpack --progress --colors --watch",
    "analyse-boundle": "webpack --mode production --profile --json > wp_stats.json && webpack-bundle-analyzer ./wp_stats.json",
    "lint": "eslint source test",
    "prepare": "npm run build",
    "test": "npm run test"
  },
  "public": true,
  "publishConfig": {
    "registry": "https://registry.unitybase.info"
  },
  "dependencies": {
    "@unitybase/codemirror-full": "^1.2.25",
    "@unitybase/cs-shared": "^5.1.9",
    "@unitybase/ub-pub": "^5.3.22",
    "@unitybase/ubm": "^5.2.18",
    "@unitybase/xlsx": "^5.1.3",
    "bluebird": "^3.5.3",
    "bluebird-q": "^2.1.1",
    "codemirror": "^5.44.0",
    "es6-promise": "^4.2.6",
    "file-saver": "^1.3.3",
    "font-awesome": "^4.7.0",
    "js-beautify": "^1.6.12",
    "jszip": "2.6.0",
    "lodash": "^4",
    "mustache": "^3.0.1",
    "mxgraph-js": "^1.0.1",
    "systemjs": "^0.20.18",
    "systemjs-hmr": "^2.0.9",
    "systemjs-plugin-css": "^0.1.32",
    "tinymce": "4.9.5"
  },
  "keywords": [
    "UnityBase",
    "UB5"
  ],
  "author": "UnityBase team (https://unitybase.info)",
  "license": "(GPL-3.0 OR LGPL-3.0 OR MPL-2.0)",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "babel-loader": "^8.0.5",
    "clean-css-cli": "^4.2.1",
    "cross-env": "^5",
    "css-loader": "^2.1.0",
    "exports-loader": "^0.7.0",
    "file-loader": "^3.0.1",
    "imports-loader": "^0.8.0",
    "mini-css-extract-plugin": "^0.5.0",
    "shx": "^0.3.2",
    "uglify-js": "^3.4.9",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3",
    "webpack-cli": "^3.2.3"
  }
}
