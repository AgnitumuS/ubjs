            .ub-logo-svg {
                position: absolute;
                width: 300px;
                height: 200px;
                top: 50%;
                left: 50%;
                margin: -150px 0 0 -150px;
            }
            .ub-logo-path {
                fill: #D0E0EC;
                stroke: #1c75bc;
                stroke-width: 1px;
                stroke-dasharray: 650;
                stroke-dashoffset: 650;
                animation: ub-logo-animate 8s ease-in-out infinite;
            }
            @keyframes ub-logo-animate {
                0% { stroke-dashoffset: 650; fill: #D0E0EC;}
                75% { stroke-dashoffset: 0; fill: #D0E0EC; }
                80% { stroke-dashoffset: 0; fill: #1c75bc; }
                100% { stroke-dashoffset: 0; fill: #1c75bc; }
            }
            .x-body {
