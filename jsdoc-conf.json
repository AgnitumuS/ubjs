{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "./base",
      "./ub",
      "./ub/modules"
    ],
    "includePattern": ".+\\.js(m|x)?$"
  },
  "plugins": [
    "plugins/markdown",
    "./ub-jsdocs/plugins/sripPFromDescription"
  ],
  "opts": {
    "template": "./ub-jsdocs",
    "readme": "./readme.md",
    "encoding": "utf8",
    "destination": "../Docs/new_all"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "buildInURL": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/",
    "buildins": [
      "array",
      "arraybuffer",
      "boolean",
      "date",
      "error",
      "function",
      "json",
      "number",
      "object",
      "regexp",
      "string",
      "null"
    ],
    "smallSourceLink": true,
    "fullTextSearch": true,
    "hideAuthor": true,
    "stylesheet": "styles/ub-jsdoc.css",
    "links": [
      {
        "text": "WEB Core API",
        "href": "../ubcore/"
      },
      {
        "text": "adminUI API",
        "href": "../adminUI/"
      }
    ],
    "googleAnalytics": "UA-66006954-1",
    "default": {
      "outputSourceFiles": true,
      "staticFiles": {
        "excludePattern": "\\.md$"
      }
    }
  }
}
