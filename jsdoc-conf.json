{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "./packages/base",
      "./packages/stubs",
      "./packages/cs-shared",
      "./packages/ub",
      "./packages/uba",
      "./packages/ub/modules",
      "./packages/ub/blobStores",
      "./packages/ubcli/bin",
      "./packages/ubcli/lib",
      "./packages/http-proxy",
      "../ub-e/packages/ubcanvas/UBCanvas.js",
      "./packages/com-bridge/UBComBridge.js",
      "./packages/mailer",
      "./packages/openid-connect/openIDConnect.js",
      "./packages/compressors/UBCompressors.js",
      "./packages/ubq",
      "./packages/ubs",
      "../ub-e/packages/odata/OData.js"
    ],
    "includePattern": ".+\\.js(m|x)?$"
  },
  "plugins": [
    "plugins/markdown",
    "./node_modules/ub-jsdoc/plugins/sripPFromDescription",
    "./node_modules/ub-jsdoc/plugins/memberOfModule.js",
    "./node_modules/ub-jsdoc/plugins/publishedTag.js"
  ],
  "opts": {
    "template": "./node_modules/ub-jsdoc",
    "readme": "./DOC-MAIN-PAGE.md",
    "encoding": "utf8",
    "destination": "./docs",
    "recurse": false,
    "tutorials": "tutorials"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "buildInURL": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/",
    "buildins": [
      "array",
      "arraybuffer",
      "boolean",
      "date",
      "error",
      "function",
      "json",
      "number",
      "object",
      "regexp",
      "string",
      "null"
    ],
    "smallSourceLink": true,
    "fullTextSearch": true,
    "hideAuthor": true,
    "stylesheet": "styles/ub-jsdoc.css",
    "links": [
      {
        "text": "WEB Core API",
        "href": "../ubcore/"
      },
      {
        "text": "adminUI API",
        "href": "../adminUI/"
      }
    ],
    "googleAnalytics": "UA-66006954-1",
    "default": {
      "outputSourceFiles": true,
      "staticFiles": {
	"include": ["./tutorials"],
        "excludePattern": "\\.md$"
      }
    }
  }
}
