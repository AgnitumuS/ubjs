			//#ifdef(%UB_USE_OCR%)
			"OCR": {
			  "dataPath": "C:\\dev\\TTesseractOCR4\\bin\\tessdata",
			  "languages": "ukr+eng"
			},
			//#endif
			"mailerConfig": {
				"targetHost": "mail.softline.main",
				"targetPort": "25",
				"fromAddr": "no-reply@softline.kiev.ua",
