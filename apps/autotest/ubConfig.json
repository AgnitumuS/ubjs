		"host": "+",
		"port": "8881",
		"requestQueueLength": 1000,
		"maxConnections": 0,
		"allowCORSFrom": "*",
		"inetPub": "./inetPub/",
		"watchFileChanges": {
			"enabled": true,
			"hashingStrategy": "ModifyDate"
		},
		"threadPoolSize": 4
	},
	"logging": {
		"path": "./logs/autotest.log",
		"levels": ["*"],
		//"levels": [],
		"stackTrackDepth": 20,
		"rotationSizeInMB": 0,
		"performanceCounters": false
	},
	"javascript": {
		"timeout": -1,
		"lifetimeInMinutes": 1000,
		"fullGCPeriod": 100,
		"maxPerEngineMemory": 512,
		"maxRecursionDepth": 32
	},
	"security": {
		"zones": [
			{
				"name":  "internal",
			    "includes": ["10.0.0.1/8"]
			}
			/*,{
				"name": "ukraine",
				// country-specific subnet masks can be downloaded from https://www.ip2location.com/free/visitor-blocker (use CIDR format)
				"includes": "#include(./zones_ukraine.json)"
			}*/
		],
//		// EE only
