		"fts": {
			"enabled": true,
			"async": false
		},
		"connections": [
			{
				"name": "main",
				"driver": "SQLite3",
				"isDefault": true,
				"dialect": "SQLite3",
				"databaseName": "./autotestDB.sqlite3",
				"supportLang": ["en", "uk"],
				"advSettings": "Synchronous=Off"
			},
			{
				"name": "ftsDefault",
				"driver": "SQLite3",
				"dialect": "SQLite3",
				"databaseName": "./autotestFTS.sqlite3",
				"supportLang": [
					"en", "uk"
				],
				//"advSettings": "Synchronous=Off"
				"advSettings": "Synchronous=Off,Tokenizer=stemka"
			},
			{
				"name": "ftsSubjectSearch",
				"driver": "SQLite3",
				"dialect": "SQLite3",
				"databaseName": "./autotestSubjectsFTS.sqlite3",
				"supportLang": [
					"en", "uk"
				],
				//"advSettings": "Synchronous=Off"
				"advSettings": "Synchronous=Off,Tokenizer=stemka"
			}
		],
		"blobStores": [
			{
				"name": "mdb",
				"storeType": "Virtual",
				"tempPath": "./_temp"
			},
			{
				"name": "simple",
				"path": "./documents/simple",
				"tempPath": "./documents/simple/_temp",
				"isDefault": true,
				"historyDepth": 2,
				"storeSize": "Medium",
				"keepOriginalFileNames": false
			},
			{
				"name": "avatars",
				"path": "./documents/avatars",
				"tempPath": "./documents/avatars/_temp",
				"historyDepth": 0,
				"storeSize": "Simple",
				"keepOriginalFileNames": false
			}
		],
