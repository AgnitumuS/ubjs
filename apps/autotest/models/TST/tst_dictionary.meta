{
  "caption": "tst_dictionary",
  "description": "ub test dictionary",
  "connectionName": "",
  "sqlAlias": "tdict",
  "cacheType": "SessionEntity",
  "descriptionAttribute": "caption",
  "privateSettings": {
    "privPrm": 12
  },
  "attributes": [
    {
      "name": "code",
      "dataType": "String",
      "size": 32,
      "caption": "Code",
      "description": "Code",
      "allowNull": false,
      "privateSettings": {
	    "attrPriv": 14
      }
    },
    {
      "name": "caption",
      "dataType": "String",
      "size": 255,
      "caption": "Caption",
      "description": "Caption",
      "allowNull": true,
      "isMultiLang": true,
      "customSettings": {
        "upload": {
          "name": "organizationCode",
          "caption": "org_organization.code",
          "additionalColumns": [
            {
              "name": "organizationName",
              "caption": "org_organization.name",
              "example": "AES-VCM Mong Duong Power"
            }
          ]
        }
      }
    },
    {
      "name": "filterValue",
      "dataType": "BigInt",
      "caption": "filterValue",
      "description": "",
      "allowNull": false
    },
    {
      "name": "currencyValue",
      "dataType": "Currency",
      "caption": "Currency Data"
    },
    {
      "name": "floatValue",
      "dataType": "Float",
      "caption": "Float Data"
    },
    {
      "name": "intValue",
      "dataType": "Int",
      "caption": "Int Data",
      "defaultValue": "0"
    },
    {
      "name": "calculated",
      "dataType": "String",
      "size": 255,
      "caption": "Назва",
      "readOnly": true,
      "mapping": [
        {
          "name": "AnsiSQL",
          "expressionType": "Expression",
          "expression": "[code] || ' ' || [caption]"
        },
        {
          "name": "MSSQL",
          "expressionType": "Expression",
          "expression": "[code] + ' ' + [caption]"
        }
      ]
    },
    {
      "name": "booleanColumn",
      "dataType": "Boolean",
      "caption": "Test boolean column",
      "allowNull": true,
      "defaultValue": "0"
    },
    {
      "name": "jsonColumn",
      "dataType": "Json",
      "caption": "Test JSON column",
      "allowNull": true,
      "defaultValue": ""
    },
    {
      "name": "doc_file",
      "dataType": "Document",
      "caption": "Attachment example"
    }
  ],
  "mixins": {
    "mStorage": {
      "simpleAudit": true,
      "safeDelete": false // should be false for autotes
    },
    "testMixin": {
      "codeDefault": "val_tst_dict"
    }
  },
  "customSettings": {"a": 1},
  "dbKeys": {
    "UIDX_CFV": {
      "code": {},
      "filterValue": {}
    }
  },
  "dbExtensions": {
		"UIDX_TST_DICT_CODE": {
			"type": "INDEX",
      "MSSQL": {
				"expression": "CREATE UNIQUE INDEX UIDX_TST_DICT_CODE on tst_dictionary(code, filterValue)"
      },
      "Oracle": {
				"expression": "CREATE UNIQUE INDEX UIDX_TST_DICT_CODE on tst_dictionary(UPPER(code), filterValue)"
      }
    }
  }
}
