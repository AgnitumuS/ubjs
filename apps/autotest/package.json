{
  "name": "autotest",
  "version": "5.1.281",
  "description": "Autotest for UnityBase",
  "repository": "https://git-pub.intecracy.com/unitybase/ubjs/tree/master/apps/autotest",
  "private": true,
  "workspaces": [
    "../../packages/*"
  ],
  "config": {
    "ubapptype": "product"
  },
  "main": "appAutotest.js",
  "scripts": {
    "profile-run": "valgrind --tool=callgrind --instr-atstart=no ub -dev",
    "profile-start": "callgrind_control -i on",
    "profile-stop": "callgrind_control -i on",
    "profile-analyse": "kcachegrind",
    "test": "echo Manually run a .\\tsql3.cmd for test",
    "test:ui": "npx testcafe chrome ./_uiautotests/*.js --screenshots ./screenshots --screenshots-on-fails",
    "ddl": "npx ubcli generateDDL -u root -autorun"
  },
