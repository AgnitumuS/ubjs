{
  "name": "docs-adminui",
  "version": "0.0.81",
  "description": "Adminui components docs",
  "repository": "https://git-pub.intecracy.com/unitybase/ubjs/tree/master/apps/docs-adminui",
  "private": true,
  "main": "ubApp.js",
  "workspaces": [
    "../../packages/*"
  ],
  "scripts": {
    "watch": "NODE_ENV=development vue-styleguidist --config ./build/styleguidist.config.js server",
    "build:prod": "rm -rf ./inetpub/build/*.* && vue-styleguidist --config ./build/styleguidist.config.js build && npm run build:after",
    "build:debugerr": "NODE_OPTIONS='--stack-trace-limit=10000' vue-styleguidist --config ./build/styleguidist.config.js build",
    "build:dev": "NODE_ENV=development vue-styleguidist --config ./build/styleguidist.config.js build && npm run build:after",
    "build:after": "ln -sf ../img ./inetpub",
    "ubapp": "ub -env ./ubConfig-dev.env",
    "dev": "gnome-terminal -x npm run ubapp; npm run watch"
  },
  "dependencies": {
    "@unitybase/adminui-pub": "^5.20.18",
    "@unitybase/adminui-reg": "^5.20.18",
    "@unitybase/adminui-vue": "^5.20.17",
    "@unitybase/cdn": "^5.20.18",
    "@unitybase/codemirror-full": "^1.3.0",
    "@unitybase/cs-shared": "^5.20.1",
    "@unitybase/ub": "^5.22.7",
    "@unitybase/ub-pub": "^5.9.12",
    "@unitybase/uba": "^5.20.18",
    "@unitybase/ubm": "^5.20.18",
    "@unitybase/ubs": "^5.20.18",
    "file-saver": "^1.3.3",
    "jdenticon": "^2.1.1",
    "js-beautify": "^1.11.0",
    "jszip": "2.6.0",
    "lodash": "^4",
    "moment": "^2.29.1",
    "mustache": "^3.0.1",
    "mxgraph-js": "^1.0.1",
    "systemjs": "^0.20.13",
    "systemjs-hmr": "^2.0.9",
    "systemjs-plugin-css": "^0.1.32",
    "vue": "^2.6.12",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@unitybase/ub-migrate": "^1.19.0",
    "@unitybase/ubcli": "^5.21.13",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.3.9",
    "vue-loader": "^15.9.6",
    "vue-styleguidist": "^4.36.1",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.28.0"
  },
  "engines": {
    "ub": "^5.0"
  },
