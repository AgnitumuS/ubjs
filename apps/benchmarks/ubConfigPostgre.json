{
	"httpServer": {
		"host": "+",
		"port": "888",
		"inetPub": ".\\img\\",
		"watchFileChanges": {
			"enabled": false,
			"hashingStrategy": "ModifyDate"
		}
	},
	"logging": {
		"levels": ["*"],
		"path": ".\\logs",
		"rotationSizeInMB": 100,
		"performanceCounters": false
	},
	"security": {
		"authenticationMethods": []
	},
	"javascript": {
		"fullGCPeriod": 100000
	},
	"application": {
		"defaultLang": "en",
		"rootHandler": "index.html",
		"domain": {
			"models": [
				{
					"name": "UB",
					"path": "./node_modules/@unitybase/ub"
				},
				{
					"name": "TPW",
					"path": "./models/tpw"
				}
			]
		},
		"connections": [
			{
				"name": "main",
				"driver": "PostgreSQL",
				"isDefault": true,
				"dialect": "PostgreSQL",
				"serverName": "postgresql://localhost:5432",
				"databaseName": "postgres",
				"userID": "ub_twp",
				"password": "ub_twp",
				"supportLang": [
					"en"
				],
				"advSettings": "LibLocation=D:\\apps\\PostgreSQL10\\bin\\libpq.dll"
			}
		],
		"blobStores": [
			{
				"name": "mdb",
				"storeType": "Virtual"
			},
			{
				"name": "avatars",
				"isDefault": true,
				"path": ".\\stores\\avatars\\",
				"historyDepth": 0,
				"storeSize": "Simple",
				"keepOriginalFileNames": false
			}
		]
	},
	"uiSettings": {
		"adminUI": {
			"applicationName": "Techempower(SQLite3)",
			"defaultPasswordForDebugOnly": "admin",
			"themeName": "UBGrayTheme",
			"endpoint": "ubadmin"
		},
		"portalUI": {}
	}
}