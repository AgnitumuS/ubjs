{
	"httpServer": {
		"host": "+",
		"port": "8881",
		"watchFileChanges": {
			"enabled": false,
			"hashingStrategy": "ModifyDate"
		},
		"threadPoolSize": 8
	},
	"logging": {
		"levels": [],
		"path": "",
		"rotationSizeInMB": 100,
		"performanceCounters": false
	},
	"security": {
		"authenticationMethods": []
	},
	"javascript": {
		"fullGCPeriod": 100
	},
	"application": {
		"defaultLang": "en",
		"domain": {
			"models": [
				{
					"path": "./node_modules/@unitybase/ub"
				},
				{
					"name": "TPW",
					"path": "./models/tpw"
				}
			]
		},
		"connections": [
			{
				"name": "main",
				"driver": "SQLite3",
				"isDefault": true,
				"dialect": "SQLite3",
				"databaseName": "./techempower.sqlite3",
				"supportLang": [
					"en"
				]
			}
		],
		"blobStores": [
			{
				"name": "mdb",
				"storeType": "Virtual",
				"tempPath": "./logs"
			}
		]
	},
	"uiSettings": {
		"adminUI": {
			"applicationName": "Techempower(SQLite3)",
			"defaultPasswordForDebugOnly": "admin",
			"themeName": "UBGrayTheme",
			"endpoint": "ubadmin"
		},
		"portalUI": {}
	}
}