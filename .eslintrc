{
  "extends": [
    "plugin:vue/recommended",
    "standard",
    "plugin:jsdoc/recommended"
  ],
  "plugins": [
    "jsdoc"
  ],
  "rules": {
    "jsdoc/require-param-description": 0,
    "jsdoc/require-returns-description": 0,
    "jsdoc/check-syntax": 1
  },
  "globals": {
    "BOUNDLED_BY_WEBPACK": true
  }
}